map r reload
map p print
map i recolor
map J zoom out
map K zoom in
map R rotate
unmap f
map f toggle_fullscreen
map [fullscreen] f toggle_fullscreen

set adjust-open "best-fit"

set pages-per-row 1

set scroll-page-aware "true"
set scroll-full-overlap 0.01
set scroll-step 100

set zoom-min 10
set guioptions ""

set synctex true
set font "Iosevka"
